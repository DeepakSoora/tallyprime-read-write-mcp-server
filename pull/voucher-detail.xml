                    <FIELD NAME="FInvR03"><SET>$$ForexValue:$AllInventoryEntries[3].Rate</SET><XMLTAG>F04</XMLTAG></FIELD>
                    <FIELD NAME="FInvA03"><SET>if $$IsDebit:$AllInventoryEntries[3].Amount then -$$NumValue:$AllInventoryEntries[3].Amount else $$NumValue:$AllInventoryEntries[3].Amount</SET><XMLTAG>F05</XMLTAG></FIELD>
                    <FIELD NAME="FInvFA03"><SET>$$ForexValue:$AllInventoryEntries[3].Amount</SET><XMLTAG>F06</XMLTAG></FIELD>
                    <FIELD NAME="FInvFR03"><SET>if $$IsEmpty:$$Currency:$AllInventoryEntries[3].Amount then 0 else $$NumValue:$AllInventoryEntries[3].Amount / $$ForexValue:$AllInventoryEntries[3].Amount</SET><XMLTAG>F07</XMLTAG></FIELD>
                    <FIELD NAME="FInvFC03"><SET>$$Currency:$AllInventoryEntries[3].Amount</SET><XMLTAG>F08</XMLTAG></FIELD>

                    <PART NAME="InvPart04"><LINES>InvLine04</LINES><REPEAT>InvLine04 : MyVoucherColl</REPEAT><SCROLLED>Vertical</SCROLLED></PART>
