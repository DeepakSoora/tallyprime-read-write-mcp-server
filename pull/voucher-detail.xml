                    <!-- ===== INVENTORY PARTS (indices 1-5) ===== -->

                    <PART NAME="InvPart01"><LINES>InvLine01</LINES><REPEAT>InvLine01 : MyVoucherColl</REPEAT><SCROLLED>Vertical</SCROLLED></PART>
                    <LINE NAME="InvLine01"><FIELDS>FInvT01,FInvN01,FInvQ01,FInvR01,FInvA01,FInvFA01,FInvFR01,FInvFC01</FIELDS><XMLTAG>ROW</XMLTAG></LINE>
                    <FIELD NAME="FInvT01"><SET>"Inventory"</SET><XMLTAG>F01</XMLTAG></FIELD>
                    <FIELD NAME="FInvN01"><SET>$AllInventoryEntries[1].StockItemName</SET><XMLTAG>F02</XMLTAG></FIELD>
                    <FIELD NAME="FInvQ01"><SET>if $$IsInwards:$AllInventoryEntries[1].BilledQty then $$Number:$AllInventoryEntries[1].BilledQty else -$$Number:$AllInventoryEntries[1].BilledQty</SET><XMLTAG>F03</XMLTAG></FIELD>
                    <FIELD NAME="FInvR01"><SET>$$ForexValue:$AllInventoryEntries[1].Rate</SET><XMLTAG>F04</XMLTAG></FIELD>
                    <FIELD NAME="FInvA01"><SET>if $$IsDebit:$AllInventoryEntries[1].Amount then -$$NumValue:$AllInventoryEntries[1].Amount else $$NumValue:$AllInventoryEntries[1].Amount</SET><XMLTAG>F05</XMLTAG></FIELD>
