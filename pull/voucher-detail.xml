<?xml version="1.0" encoding="utf-8"?>
<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>MyTallyLiveReport</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <SVFROMDATE>{fromDate}</SVFROMDATE>
                <SVTODATE>{toDate}</SVTODATE>
                <nunjuck>if targetCompany</nunjuck>
                <SVCURRENTCOMPANY>{{targetCompany | escape}}</SVCURRENTCOMPANY>
                <nunjuck>endif</nunjuck>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <REPORT NAME="MyTallyLiveReport">
                        <FORMS>MyForm</FORMS>
                    </REPORT>
                    <FORM NAME="MyForm">
                        <PARTS>LedPart01,LedPart02,LedPart03,LedPart04,LedPart05,LedPart06,LedPart07,LedPart08,LedPart09,LedPart10,InvPart01,InvPart02,InvPart03,InvPart04,InvPart05</PARTS>
                        <XMLTAG>DATA</XMLTAG>
                    </FORM>

                    <!-- ===== LEDGER PARTS (indices 1-10) ===== -->

                    <PART NAME="LedPart01"><LINES>LedLine01</LINES><REPEAT>LedLine01 : MyVoucherColl</REPEAT><SCROLLED>Vertical</SCROLLED></PART>
                    <LINE NAME="LedLine01"><FIELDS>FLedT01,FLedN01,FLedQ01,FLedR01,FLedA01,FLedFA01,FLedFR01,FLedFC01</FIELDS><XMLTAG>ROW</XMLTAG></LINE>
                    <FIELD NAME="FLedT01"><SET>"Ledger"</SET><XMLTAG>F01</XMLTAG></FIELD>
                    <FIELD NAME="FLedN01"><SET>$AllLedgerEntries[1].LedgerName</SET><XMLTAG>F02</XMLTAG></FIELD>
                    <FIELD NAME="FLedQ01"><SET>0</SET><XMLTAG>F03</XMLTAG></FIELD>
                    <FIELD NAME="FLedR01"><SET>0</SET><XMLTAG>F04</XMLTAG></FIELD>
                    <FIELD NAME="FLedA01"><SET>if $$IsDebit:$AllLedgerEntries[1].Amount then -$$NumValue:$AllLedgerEntries[1].Amount else $$NumValue:$AllLedgerEntries[1].Amount</SET><XMLTAG>F05</XMLTAG></FIELD>
                    <FIELD NAME="FLedFA01"><SET>$$ForexValue:$AllLedgerEntries[1].Amount</SET><XMLTAG>F06</XMLTAG></FIELD>
                    <FIELD NAME="FLedFR01"><SET>if $$IsEmpty:$$Currency:$AllLedgerEntries[1].Amount then 0 else $$NumValue:$AllLedgerEntries[1].Amount / $$ForexValue:$AllLedgerEntries[1].Amount</SET><XMLTAG>F07</XMLTAG></FIELD>
                    <FIELD NAME="FLedFC01"><SET>$$Currency:$AllLedgerEntries[1].Amount</SET><XMLTAG>F08</XMLTAG></FIELD>

