                        <TYPE>Voucher</TYPE>
                        <FETCH>AllLedgerEntries,AllInventoryEntries</FETCH>
                        <FILTER>FiltType,FiltNum,FiltCanc,FiltOpt</FILTER>
                    </COLLECTION>
                    <SYSTEM TYPE="Formulae" NAME="FiltType">$VoucherTypeName = "{voucherType}"</SYSTEM>
                    <SYSTEM TYPE="Formulae" NAME="FiltNum">$VoucherNumber = "{voucherNumber}"</SYSTEM>
                    <SYSTEM TYPE="Formulae" NAME="FiltCanc">NOT $IsCancelled</SYSTEM>
                    <SYSTEM TYPE="Formulae" NAME="FiltOpt">NOT $IsOptional</SYSTEM>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>
