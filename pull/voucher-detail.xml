                    <FIELD NAME="FInvFR02"><SET>if $$IsEmpty:$$Currency:$AllInventoryEntries[2].Amount then 0 else $$NumValue:$AllInventoryEntries[2].Amount / $$ForexValue:$AllInventoryEntries[2].Amount</SET><XMLTAG>F07</XMLTAG></FIELD>
                    <FIELD NAME="FInvFC02"><SET>$$Currency:$AllInventoryEntries[2].Amount</SET><XMLTAG>F08</XMLTAG></FIELD>

