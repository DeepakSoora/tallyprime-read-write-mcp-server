                    <FIELD NAME="FInvT04"><SET>"Inventory"</SET><XMLTAG>F01</XMLTAG></FIELD>
                    <FIELD NAME="FInvN04"><SET>$AllInventoryEntries[4].StockItemName</SET><XMLTAG>F02</XMLTAG></FIELD>
                    <FIELD NAME="FInvQ04"><SET>if $$IsInwards:$AllInventoryEntries[4].BilledQty then $$Number:$AllInventoryEntries[4].BilledQty else -$$Number:$AllInventoryEntries[4].BilledQty</SET><XMLTAG>F03</XMLTAG></FIELD>
                    <FIELD NAME="FInvR04"><SET>$$ForexValue:$AllInventoryEntries[4].Rate</SET><XMLTAG>F04</XMLTAG></FIELD>
                    <FIELD NAME="FInvA04"><SET>if $$IsDebit:$AllInventoryEntries[4].Amount then -$$NumValue:$AllInventoryEntries[4].Amount else $$NumValue:$AllInventoryEntries[4].Amount</SET><XMLTAG>F05</XMLTAG></FIELD>
                    <FIELD NAME="FInvFA04"><SET>$$ForexValue:$AllInventoryEntries[4].Amount</SET><XMLTAG>F06</XMLTAG></FIELD>
                    <FIELD NAME="FInvFR04"><SET>if $$IsEmpty:$$Currency:$AllInventoryEntries[4].Amount then 0 else $$NumValue:$AllInventoryEntries[4].Amount / $$ForexValue:$AllInventoryEntries[4].Amount</SET><XMLTAG>F07</XMLTAG></FIELD>
