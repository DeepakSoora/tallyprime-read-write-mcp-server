                    <FIELD NAME="FInvT02"><SET>"Inventory"</SET><XMLTAG>F01</XMLTAG></FIELD>
                    <FIELD NAME="FInvN02"><SET>$AllInventoryEntries[2].StockItemName</SET><XMLTAG>F02</XMLTAG></FIELD>
                    <FIELD NAME="FInvQ02"><SET>if $$IsInwards:$AllInventoryEntries[2].BilledQty then $$Number:$AllInventoryEntries[2].BilledQty else -$$Number:$AllInventoryEntries[2].BilledQty</SET><XMLTAG>F03</XMLTAG></FIELD>
                    <FIELD NAME="FInvR02"><SET>$$ForexValue:$AllInventoryEntries[2].Rate</SET><XMLTAG>F04</XMLTAG></FIELD>
                    <FIELD NAME="FInvA02"><SET>if $$IsDebit:$AllInventoryEntries[2].Amount then -$$NumValue:$AllInventoryEntries[2].Amount else $$NumValue:$AllInventoryEntries[2].Amount</SET><XMLTAG>F05</XMLTAG></FIELD>
