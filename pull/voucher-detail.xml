                    <FIELD NAME="FLedFA06"><SET>$$ForexValue:$AllLedgerEntries[6].Amount</SET><XMLTAG>F06</XMLTAG></FIELD>
                    <FIELD NAME="FLedFR06"><SET>if $$IsEmpty:$$Currency:$AllLedgerEntries[6].Amount then 0 else $$NumValue:$AllLedgerEntries[6].Amount / $$ForexValue:$AllLedgerEntries[6].Amount</SET><XMLTAG>F07</XMLTAG></FIELD>
                    <FIELD NAME="FLedFC06"><SET>$$Currency:$AllLedgerEntries[6].Amount</SET><XMLTAG>F08</XMLTAG></FIELD>

                    <PART NAME="LedPart07"><LINES>LedLine07</LINES><REPEAT>LedLine07 : MyVoucherColl</REPEAT><SCROLLED>Vertical</SCROLLED></PART>
                    <LINE NAME="LedLine07"><FIELDS>FLedT07,FLedN07,FLedQ07,FLedR07,FLedA07,FLedFA07,FLedFR07,FLedFC07</FIELDS><XMLTAG>ROW</XMLTAG></LINE>
                    <FIELD NAME="FLedT07"><SET>"Ledger"</SET><XMLTAG>F01</XMLTAG></FIELD>
                    <FIELD NAME="FLedN07"><SET>$AllLedgerEntries[7].LedgerName</SET><XMLTAG>F02</XMLTAG></FIELD>
                    <FIELD NAME="FLedQ07"><SET>0</SET><XMLTAG>F03</XMLTAG></FIELD>
                    <FIELD NAME="FLedR07"><SET>0</SET><XMLTAG>F04</XMLTAG></FIELD>
                    <FIELD NAME="FLedA07"><SET>if $$IsDebit:$AllLedgerEntries[7].Amount then -$$NumValue:$AllLedgerEntries[7].Amount else $$NumValue:$AllLedgerEntries[7].Amount</SET><XMLTAG>F05</XMLTAG></FIELD>
                    <FIELD NAME="FLedFA07"><SET>$$ForexValue:$AllLedgerEntries[7].Amount</SET><XMLTAG>F06</XMLTAG></FIELD>
                    <FIELD NAME="FLedFR07"><SET>if $$IsEmpty:$$Currency:$AllLedgerEntries[7].Amount then 0 else $$NumValue:$AllLedgerEntries[7].Amount / $$ForexValue:$AllLedgerEntries[7].Amount</SET><XMLTAG>F07</XMLTAG></FIELD>
